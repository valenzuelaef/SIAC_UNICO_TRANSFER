@{
    ViewBag.Title = "Index";
    Layout = null;
}

<style>
    .check-text {
        text-align: center;
        font-weight: bold;
        font-size: 16px;
        line-height: 19px;
        padding: 15px;
    }
</style>
 
<div class="container-fluid" id="divIndex">
    <div class="row">
        <div id="divCustomerDataView" class="col-md-3 column-layout"></div>
        <div id="containerTransfer">
            <div class="col-md-9 column-layout" style="padding: 0">
                <div class="bg-container">
                    <div class="row">
                        <div class="col-md-8">
                            <div class="box-title-transfer">
                                <div id="divTransferInternal" class="divSelect-transfer">
                                    <div>
                                        <span class="ico_traslado_interno"></span>
                                        <span class="package">TRASLADO INTERNO</span>
                                    </div>
                                </div>
                            </div>
                            <div class="box-title-transfer">
                                <div id="divTransferExternal" class="divNoSelect-transfer">
                                    <div>
                                        <span class="ico_traslado_externo"></span>
                                        <span class="package">TRASLADO EXTERNO</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div id="containerTransfer_operacion" style="border: 1px solid;margin-top:0px; border-color:#C4C4C4; padding: 10px;">
                                <div id="divInternalTransfer">

                                </div>
                                <div id="divExternalTransfer">
                                    <div class="transaction-container-step">
                                        <div class="transaction-process-step">
                                            <div class="process-row-step nav nav-tabs">
                                            </div>
                                        </div>
                                        <div class="transaction-progress-Steps">
                                            <div class="transaction-process transaction-bar-step">
                                                <div id="prog" class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="transactionContent" class="tab-content">
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="footer-info-sot" style="background-color:#747D84;width:100%;height:56px;border-radius:5px;display:none;color:#FFF; text-align:center;padding:10px  ;font-size:25px;border-radius : 0px 0px 20px 20px;">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>

@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
<script src="~/Areas/Transfer/Scripts/Index.js"></script>
